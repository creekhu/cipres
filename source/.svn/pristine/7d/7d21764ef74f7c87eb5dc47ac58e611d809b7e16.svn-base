		      <value>7</value>
		      <label>gray value: 7 (lowercase)</label>
		      <value>8</value>
		      <label>gray value: 8 = darkest (lowercase)</label>
		      <value>R</value>
		      <label>(R) Red</label>
		      <value>r</value>
		      <label>(r) Red</label>
		      <value>G</value>
		      <label>(G) Green</label>
		      <value>g</value>
		      <label>(g) Green</label>
		      <value>L</value>
		      <label>(L) Blue</label>
		      <value>l</value>
		      <label>(l) Blue</label>
		      <value>Y</value>
		      <label>(Y) Yellow</label>
		      <value>y</value>
