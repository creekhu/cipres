		  <name>identical_foreground</name>
		  <attributes>

		    <prompt>Foreground for identical residues (lowercase choices mean lowercase letters in the sequence)</prompt>
		    <format>
		      <language>perl</language>
		      <code>"$value\\n"</code>
		    </format>
		    <vdef><value>W</value></vdef>
		    <group>23</group>
		    <vlist>
		      <value>B</value>
