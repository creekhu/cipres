		      <language>perl</language>
		      <code>"$value\\n"</code>
		    </format>
		    <vdef><value>W</value></vdef>
		    <group>20</group>
		    <vlist>
		      <value>B</value>
		      <label>(B) black</label>
		      <value>W</value>
		      <label>(W) white</label>
		      <value>1</value>
		      <label>gray value: 1</label>
		      <value>2</value>
		      <label>gray value: 2</label>
		      <value>3</value>
		      <label>gray value: 3</label>
		      <value>4</value>
		      <label>gray value: 4 (darkest)</label>
		      <value>R</value>
		      <label>(R) Red</label>
		      <value>G</value>
		      <label>(G) Green</label>
		      <value>L</value>
		      <label>(L) Blue</label>
		      <value>Y</value>
		      <label>(Y) Yellow</label>
		      <value>M</value>
		      <label>(M) Magenta</label>
		      <value>C</value>
		      <label>(C) Cyan</label>
		    </vlist>
		    <precond>
		      <language>perl</language>
		      <code>$output_format ne "b"</code>
		    </precond>
		    <paramfile>params</paramfile>

		  </attributes>
		</parameter>

