		      <value>b</value>
		      <label>(b) black</label>
		      <value>W</value>
		      <label>(W) white</label>
		      <value>w</value>
		      <label>(w) white</label>
		      <value>1</value>
		      <label>gray value: 1</label>
		      <value>2</value>
